        <source_script><![CDATA[answer = (function transformEntry(source) {


    var hardware = new GlideRecord('alm_hardware');
    if (source.vassetsourcekey) {
        hardware.addQuery('sys_id', source.vassetsourcekey.trim());
    } else if (source.serialnumber) {
        hardware.addQuery('serial_number', source.serialnumber.trim());
    }
    hardware.query();
    if (hardware.next()) {
        return hardware.sys_id.toString();
    } else {
        return -1;
    }

})(source);]]></source_script>
