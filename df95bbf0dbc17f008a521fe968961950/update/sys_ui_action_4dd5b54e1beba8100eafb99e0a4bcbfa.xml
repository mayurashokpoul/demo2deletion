        <name>Reset Retry Count</name>
        <onclick>resetCount()</onclick>
        <order>100</order>
        <script><![CDATA[function resetCount() {
    var selSysIds = g_list.getChecked();
	var ajaxHelper = new GlideAjax('IMITADLocationUtils');
	ajaxHelper.addParam('sysparm_name', 'resetCount');
    ajaxHelper.addParam('sysparm__sys_ids', selSysIds);
	ajaxHelper.getXML(displaResult);
 
function displaResult(response) {
  var answer = response.responseXML.documentElement.getAttribute("answer");
  var result=JSON.parse(answer);

	if(result.status=="success"){
		alert("Retry count has been successfully reset");
	}else{
		alert("Seleted Records have Retry count less than 5.Please select records reached max count 5.Please find the records: "+result.countlessArr);
	}
  

}
}
]]></script>
