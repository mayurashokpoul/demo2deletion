		<div style="float: left;  padding-top: 8px;">
			<label for="input1" id="inputLabel" >Schedule Date Time : </label> </div>
	<g:ui_date_time style=" width: 250px;" id="schedule_date" name ="schedule_date" value="" onChange="checkMandatory()"  />
		
		</div>
		
		<div class="modal-footer">
	<span class="pull-right"> 
		<div class="pull-right form-group"  id="dialog-btns">
		<g:dialog_buttons_ok_cancel ok="return actionOK();" cancel="return Cancel();"/>
		</div>
	</span>
</div>
