<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_page">
    <sys_ui_page action="DELETE">
        <category>general</category>
        <client_script><![CDATA[document.getElementById('inputLabel').setAttribute("class", "required");

function checkMandatory() {
    if (document.getElementById('schedule_date').value != "") {
        document.getElementById('inputLabel').setAttribute("class", "notrequired");
    } else {
        document.getElementById('inputLabel').setAttribute("class", "required");
    }
}

function onCancel() {
    var c = gel('cancelled');
	c.value = "true";
	GlideDialogWindow.get().destroy();
	return false;
}

function actionOK() {
    try {
        var scheduleDateTime = gel("schedule_date").value;

        if (!scheduleDateTime) {
            alert('Please enter "Schedule Date Time" value to initiate Erasure');
            return;
        } else {
            var ga = new GlideAjax('x_inmis_im_itad.ErasureUtils');
            ga.addParam('sysparm_name', 'checkDateDiff');
            ga.addParam('sysparm_scheduleDateTime', scheduleDateTime);
            ga.getXMLWait();

            var answer = ga.getAnswer();

            //alert('answer: ' + answer);
            if (answer.toString() == 'true') {
                alert("Please select 'Shedule Date Time' after 5 day's from today's date");
                return false;
            } else {

                alert('Selected assets has been schedueld for Erasure');

                // GlideDialogWindow.get().destroy(); //Close the dialog window
				
            }
        }
    } catch (e) {
        alert('>>>Exception occured: ' + e);
    }
}]]></client_script>
